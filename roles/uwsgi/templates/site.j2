server {
    listen 80;
    server_name {{ app_name }}.matthewkrohn.net;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/uwsgi/{{ app_name }};
    }

    location / {
        include         uwsgi_params;
        uwsgi_pass      unix:/run/uwsgi/{{ app_name }}.sock;
    }
}
